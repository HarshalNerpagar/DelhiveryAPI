{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "prisma/schema.prisma",
            "dev.db" 
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/server.js"
      }
    ]
  }