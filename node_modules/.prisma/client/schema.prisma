generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Shipment {
  id            String   @id @default(uuid())
  waybill       String
  orderId       String   @unique
  status        String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  pickupAddress Json
  shipmentData  Json
  responseData  Json
}
